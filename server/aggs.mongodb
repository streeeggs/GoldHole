var echo_message_by_date = [
    //...first_said, 
    {      
      "$group": {
          "_id": {
              "text": { "$toLower": "$message.text" },
              "name": "$name"
          },
          "minDate": { "$min" : "$message.time"  }
      },
    },
    { 
        "$sort": { "minDate" : 1, "_id.name": 1 }
    },
    {
        "$group": {
            "_id": "$_id.text",
            "out": { "$first": "$$ROOT" }
        }
    },
    {
      "$project": {
        "name": "$out._id.name",
        "date": "$out.minDate",
        "text": "$out._id.text"
      }
    },
    {
      "$group": {
        "_id": {
          "user": "$name",
          "month": { "$dateToString": { format: "%Y-%m", date: "$date" } },
        },
        "items": {
            $push: "$text" 
        },
      }
    },
    {
      "$project": {
        "user": "$_id.user",
        "month": "$_id.month",
        "items": { "$setUnion": [ "$items", "$items" ] },
      }
    },
    {
      "$project": {
        "user": "$_id.user",
        "month": "$_id.month",
        "items": "$items",
        "count": { "$size": "$items" },
      }
    },
    { 
        "$sort": { "count" : -1, "user": 1 }
    },
 ]

