var by_golds = [
  {      
    "$group": {
        "_id": {
          "title": "$title",
          "text": "$message.text"
        },
        "agg": { "$addToSet" : "$title"  }
      },
  },
  {
    "$sortByCount": "$_id.title"
  },
  {
      "$project": {
        "Title": "$_id",
        "Count": "$count"
      }
  },
  {
      "$sort": { "Count": -1, "Title": 1}
  }
]

